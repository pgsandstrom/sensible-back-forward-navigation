{
  "name": "sensible-back-forward-navigation",
  "displayName": "Sensible back and forward navigation",
  "description": "Improve 'go back' and 'go forward' command is vscode. Movement is grouped in a sensible way.",
  "version": "0.3.0",
  "publisher": "codeandstuff",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pgsandstrom/sensible-back-forward-navigation.git"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Sensible back forward navigation",
      "properties": {
        "sensible-back-forward-navigation.centerOnMovement": {
          "type": "boolean",
          "default": true,
          "description": "When navigating to a place outside the view, should we always put the edit in the center of the screen?"
        },
        "sensible-back-forward-navigation.logDebug": {
          "type": "boolean",
          "default": false,
          "description": "Log a bunch of stuff to the console. Use if submitting a bug report."
        }
      }
    },
    "commands": [
      {
        "command": "sensible-back-forward-navigation.goBack",
        "title": "Go back sensibly"
      },
      {
        "command": "sensible-back-forward-navigation.goForward",
        "title": "Go forward sensibly"
      }
    ],
    "keybindings": [
      {
        "command": "sensible-back-forward-navigation.goBack",
        "key": "alt+h",
        "mac": "alt+h"
      },
      {
        "command": "sensible-back-forward-navigation.goForward",
        "key": "alt+l",
        "mac": "alt+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write \"src/**/*.{json,md,ts,tsx,css,scss}\"",
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:js": "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix src",
    "lint:ts": "tslint --fix src/**/*.ts{,x}",
    "package": "vsce package",
    "pre-commit": "lint-staged",
    "pre-push": "tsc --noEmit",
    "// TODO": "Fix a cool way to run the tests on pre-push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  },
  "lint-staged": {
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --fix",
      "tslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.9",
    "@types/vscode": "1.51.0",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "cross-env": "7.0.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "glob": "7.1.6",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "mocha": "8.2.1",
    "prettier": "2.2.0",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "vsce": "1.81.1",
    "vscode-test": "1.4.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  }
}
